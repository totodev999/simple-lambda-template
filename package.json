{
  "name": "simple-lambda-template",
  "version": "1.0.0",
  "description": "simple lambda template.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc -build",
    "bundle:sample": "esbuild functions/sample/handler.ts --bundle --outfile=./functions/sample/index.js --platform=node",
    "zip:sample": "zip -j ./sample functions/sample/index.js && rm ./functions/sample/index.js",
    "build:sample": "npm run build && npm run bundle:sample && npm run zip:sample"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.7"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "esbuild": "^0.24.0",
    "tsc": "^2.0.4",
    "typescript": "^5.6.3"
  }
}
